{
  "manifest_version": 2,

  "name": "Proxy setup",
  "description": "This extension allows the user to change Chrome proxy in incognito sessions.",
  "version": "2.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "incognito" : "split",
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "proxy"
  ]
}
